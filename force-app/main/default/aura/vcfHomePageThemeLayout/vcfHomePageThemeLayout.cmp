<aura:component implements="forceCommunity:themeLayout" access="global" >
    <aura:attribute name="search" type="Aura.Component[]"/>
    <aura:attribute name="sidebarFooter" type="Aura.Component[]"/>
    <aura:attribute name="ProfileMenu" type="Aura.Component[]"/>
    <aura:attribute name="navBar" type="Aura.Component[]"/>

    <div class="slds-grid slds-wrap">
        <div class="slds-col slds-size_1-of-1">
            {!v.navBar}
        </div>
        <div class="slds-col slds-size_1-of-1">
            {!v.search}
        </div>
        <div class="slds-col slds-size_1-of-1">
            {!v.body}
            <div class="slds-col headerTop">
                <div class="slds-grid">
                    <div class="slds-col slds-size_8-of-12">
                        SEPTEMBER COMPENSATION FUND
                    </div>
                </div>
            </div>
        </div>
        <div class="slds-col slds-size_1-of-1">
            {!v.sidebarFooter}
        </div>
        <div class="slds-col slds-size_1-of-1">
            {!v.ProfileMenu}
        </div>
    </div>

    

    
</aura:component>